{
  "name": "canner-slate-editor-pkgs",
  "version": "1000000.0.0",
  "description": "Another rich text editor using Slate framework",
  "author": {
    "name": "chilijung",
    "email": "chilijung@gmail.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "slate",
    "editor",
    "icons"
  ],
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "antd": "3.9.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "cross-env": "^2.0.1",
    "css-loader": "^0.28.10",
    "enzyme": "3.5.1",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.80.0",
    "github-markdown-css": "^2.10.0",
    "husky": "^1.0.0-rc.13",
    "immutable": "^4.0.0-rc.9",
    "jest": "^23.5.0",
    "js-beautify": "^1.7.5",
    "lerna": "^3.2.1",
    "less": "2.3.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "markup-it": "^7.0.3",
    "prettier": "^1.9.2",
    "prismjs": "^1.13.0",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.4.2",
    "read-metadata": "^1.0.0",
    "regenerator-runtime": "^0.12.1",
    "slate": "^0.40.2",
    "slate-edit-blockquote": "0.6.1",
    "slate-edit-code": "^0.15.5",
    "slate-edit-list": "^0.12.1",
    "slate-edit-table": "^0.18.0",
    "slate-html-serializer": "^0.7.2",
    "slate-hyperscript": "^0.10.2",
    "slate-prism": "^0.5.0",
    "slate-react": "^0.18.5",
    "slate-simulator": "^0.4.61",
    "slate-trailing-block": "^0.5.0",
    "style-loader": "^0.20.3",
    "styled-components": "^3.2.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "scripts": {
    "build": "lerna exec --parallel -- babel src -d lib --copy-files",
    "build:watch": "lerna exec --parallel -- babel src -d lib --copy-files -w & npm run build:watch-test",
    "build:watch-test": "babel ./test/src -d ./test/lib --copy-files -w",
    "clear": "lerna exec --parallel -- rm -rf lib",
    "test": "lerna exec --parallel -- npm test",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --mode development",
    "build:docs": "npm run build && ./node_modules/.bin/webpack --config webpack.config.prod.js --mode production",
    "jest": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^test/(.*)$": "<rootDir>/test/lib/$1",
      "^.+\\.(css|scss|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testRegex": "(\\.|/)(test|spec)\\.jsx?$",
    "testURL": "http://localhost/"
  },
  "workspaces": [
    "packages/**"
  ],
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "Canner/canner-slate-editor",
  "license": "MIT",
  "dependencies": {}
}
